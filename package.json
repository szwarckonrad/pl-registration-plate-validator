{
  "name": "pl-registration-plate-validator",
  "version": "1.0.0",
  "description": "Validate provided Polish registration plate number.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "cover": "jest --coverage",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szwarckonrad/pl-registration-plate-validator.git"
  },
  "keywords": [
    "validator",
    "typescript",
    "registration",
    "plate"
  ],
  "author": "Konrad Szwarc <https://github.com/szwarckonrad/>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/szwarckonrad/pl-registration-plate-validator/issues"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/index.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "homepage": "https://github.com/szwarckonrad/pl-registration-plate-validator#readme",
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^10.3.0",
    "coveralls": "^3.0.0",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "@types/lodash": "^4.14.106",
    "lodash": "^4.17.5"
  }
}
